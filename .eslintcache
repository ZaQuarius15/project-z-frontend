filePath":"60","messages":"61","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/App.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/components/NoteCard.js",[],["62","63"],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/components/NotesContainer.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/components/NewNoteForm.js",["64"],"import React, { Component } from 'react'\nimport { connect } from 'react-redux'\nimport { addNote } from '../actions/index.js'\nimport { Form } from 'semantic-ui-react'\nimport { Redirect } from 'react-router'\n\nconst options = [\n    { key: 'm', text: 'Scholarly', value: 'scholarly' },\n    { key: 'f', text: 'Netflix Show Ideas', value: 'netflix' },\n    { key: 'o', text: \"Just 'Cuz\" , value: 'fun' },\n  ]\n\nexport class NewNoteForm extends Component {\n\n    state = {\n        title: '',\n        image: '',\n        description: '',\n        category: '',\n        redirect: false\n    }\n\n    handleChange = (e) => {\n        this.setState({\n            [e.target.name]: e.target.value\n        })\n    }\n\n    handleSubmit = (e) => {\n        e.preventDefault();\n\n        const newNote = {\n            title: this.state.title,\n            image: this.state.image,\n            description: this.state.description,\n            user_id: 1\n        }\n        \n\n        const reqObj = {\n            method: 'POST',\n            headers: {\n                'Content-Type': 'application/json'\n            },\n            body: JSON.stringify(newNote)\n        }\n        fetch('http://localhost:3000/notes', reqObj)\n        .then(resp => resp.json())\n        .then(newNote => {\n            this.props.addNote(newNote)\n            this.setState({\n                title: '',\n                image: '',\n                description: '',\n                category: '',\n                redirect: true\n            })\n        })\n    }\n\n\n    render() {\n        const { value } = this.state\n        if (this.state.redirect === true ) {\n            return(\n                <Redirect to='/notes'/>\n            )\n        }\n        return (\n            <Form onSubmit={this.handleSubmit}>\n            <Form.Group widths='equal'>\n              <Form.Input onChange={this.handleChange} label='Title' value={this.state.title} name='title' placeholder='Note title...' />\n              <Form.Input onChange={this.handleChange} label='Image' value={this.state.image} name='image' placeholder='Image URL...' />\n              <Form.Select\n                fluid\n                name='category'\n                label='Category'\n                options={options}\n                placeholder='Category...'\n              />\n            </Form.Group>\n            <Form.TextArea onChange={this.handleChange} label='Note Description' name='description' value={this.state.description} placeholder=\"Here's to the blank page...\" />\n            <Form.Button type='submit'>Submit</Form.Button>\n          </Form>\n        )\n    }\n}\n\nexport default connect(null, { addNote })(NewNoteForm)\n","/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/index.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/reducers/noteReducer.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/actions/index.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/components/NavBar.js",["65","66"],"\nimport React, { Component } from 'react'\nimport { NavLink, withRouter } from 'react-router-dom';\nimport { Input, Menu } from 'semantic-ui-react'\n\nexport class NavBar extends Component {\n  state = { \n    activeItem: 'home'\n  }\n\n  handleItemClick = (e, { name }) => {\n    this.setState({\n      activeItem: name\n    })\n  }\n\n  render() {\n    const { activeItem } = this.state\n    return (\n      <Menu>\n        <Menu.Item\n          as={NavLink}\n          to='/notes'\n          name='home'\n          active={activeItem === 'home'}\n          onClick={this.handleItemClick}\n        >\n          Home\n        </Menu.Item>\n\n        <Menu.Item\n          as={NavLink}\n          to='/new-note'\n          name='new-note'\n          active={activeItem === 'new-note'}\n          onClick={this.handleItemClick}\n        >\n          Create New Note\n        </Menu.Item>\n        <Menu.Item\n          as={NavLink}\n          to='/login'\n          name='logout'\n          active={activeItem === 'logout'}\n          onClick={this.handleItemClick}\n        >\n          Logout\n        </Menu.Item>\n        <Menu.Menu position='right'>\n        <div className='ui right aligned category search item'>\n          <div className='ui transparent icon input'>\n            <input\n              className='prompt'\n              type='text'\n              placeholder='Search Notes...'\n            />\n            <i className='search link icon' />\n          </div>\n          <div className='results' />\n        </div>\n      </Menu.Menu>\n      </Menu>\n    )\n  }\n}\n\nexport default NavBar\n","/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/reducers/index.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/reducers/authReducer.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/components/Login.js",["67","68"],"import React, { Component } from 'react'\nimport { Button, Form, Grid, Header, Image, Message, Segment } from 'semantic-ui-react'\n\nexport class Login extends Component {\n\n    state ={\n        username: '',\n        password: ''\n    }\n\n    handleChange = e => {\n        this.setState({\n            [e.target.name]: e.target.value\n        })\n    }\n\n    render() {\n        return (\n            <div>\n                <h1>Welcome To Notes!!</h1>\n                <Grid textAlign='center' style={{ height: '100vh' }} verticalAlign='middle'>\n                    <Grid.Column style={{ maxWidth: 450 }}>\n                        <Header as='h2' color='blue' textAlign='center'>\n                            Log-in to your account\n                        </Header>\n                        <Form size='large'>\n                            <Segment stacked>\n                            <Form.Input onChange={this.handleChange} icon='user' name='username' iconPosition='left' placeholder='Username' />\n                            <Form.Input\n                                onChange={this.handleChange}\n                                icon='lock'\n                                iconPosition='left'\n                                placeholder='Password'\n                                name='password'\n                                type='password'\n                            />\n\n                            <Button color='blue' fluid size='large'>\n                                Login\n                            </Button>\n                            </Segment>\n                        </Form>\n                    </Grid.Column>\n                </Grid>\n            </div>\n        )\n    }\n}\n\nexport default Login\n","/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/components/NoteShowPage.js",["69"],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/reducers/showNoteReducer.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/components/EditNoteForm.js",["70"],{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","severity":1,"message":"76","line":63,"column":17,"nodeType":"77","messageId":"78","endLine":63,"endColumn":22},{"ruleId":"75","severity":1,"message":"79","line":3,"column":19,"nodeType":"77","messageId":"78","endLine":3,"endColumn":29},{"ruleId":"75","severity":1,"message":"80","line":4,"column":10,"nodeType":"77","messageId":"78","endLine":4,"endColumn":15},{"ruleId":"75","severity":1,"message":"81","line":2