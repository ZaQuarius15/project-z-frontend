te)\n        }\n        fetch('http://localhost:3000/notes', reqObj)\n        .then(resp => resp.json())\n        .then(newNote => {\n            this.props.addNote(newNote)\n            this.setState({\n                title: '',\n                image: '',\n                description: '',\n                category: '',\n                redirect: true\n            })\n        })\n    }\n\n\n    render() {\n        const { value } = this.state\n        if (this.state.redirect === true ) {\n            return(\n                <Redirect to='/notes'/>\n            )\n        }\n        return (\n            <Form onSubmit={this.handleSubmit}>\n            <Form.Group widths='equal'>\n              <Form.Input onChange={this.handleChange} label='Title' value={this.state.title} name='title' placeholder='Note title...' />\n              <Form.Input onChange={this.handleChange} label='Image' value={this.state.image} name='image' placeholder='Image URL...' />\n              <Form.Select\n                fluid\n                name='category'\n                label='Category'\n                options={options}\n                placeholder='Category...'\n              />\n            </Form.Group>\n            <Form.TextArea onChange={this.handleChange} label='Note Description' name='description' value={this.state.description} placeholder=\"Here's to the blank page...\" />\n            <Form.Button type='submit'>Submit</Form.Button>\n          </Form>\n        )\n    }\n}\n\nexport default connect(null, { addNote })(NewNoteForm)\n","/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/index.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/reducers/noteReducer.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/actions/index.js",[],"/Users/JohnZacharyCarlson/mod-4-project/project-z-frontend/src/components/NavBar.js",["69","70"],"\nimport React, { Component } from 'react'\nimport { NavLink, withRouter } from 'react-router-dom